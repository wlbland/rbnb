<div class="container">
  <div class="row">
    <div class="col-sm-offset-4 col-sm-4">
      <h2 class="text-centre">Bookings.</h2>
      <%= simple_form_for [@car, @booking] do |f| %>
      <%= f.input :start_date %>
      <%= f.input :end_date %>
      <%= f.submit "Book", class: "btn btn-success" %>
      <% end %>
    </div>
  </div>
</div>





